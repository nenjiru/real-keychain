{
    "name": "Real Keychain",
    "version": "0.3",
    "description": "キーイベントでKOEBUにログイン",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": ["https://secure.koebu.com/*", "http://koebu.com/*"],
            "js": ["init.js"]
        }
    ],
    "manifest_version": 2
}
